{
    "name": "Air-Temperature-across-Singapore",
    "url": "https://api.data.gov.sg/v1/environment/air-temperature",
    "csv_path": "~/data/Airflow/playground/data/SG-city-environment/Air-Temperature-across-Singapore",
    "data_path": [
		"['items'][0]['timestamp']",
		"['metadata']['reading_unit']"
    ],
	"nested_data": {
    	"path_1": "['metadata']['stations']",
    	"fields_1": ["id", "device_id", "name", "location.latitude", "location.longitude"],
    	"merge_key_1": "id",
    	"path_2": "['items'][0]['readings']",
		"fields_2": ["station_id", "value"],
    	"merge_key_2": "station_id"
	},
    "freq_minute": 1,
    "remaining_minute": 1
}

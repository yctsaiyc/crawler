{
    "name": "Relative-Humidity-across-Singapore",
    "url": "https://api.data.gov.sg/v1/environment/relative-humidity",
    "csv_path": "~/opt/airflow/playground/data/SG-city-environment/Relative-Humidity-across-Singapore",
    "common_data_path": [
        "['metadata']['reading_unit']",
        "['items'][0]['timestamp']"
    ],
    "individual_data": {
        "path_1": "['metadata']['stations']",
        "fields_1": [
            "id",
            "name",
            "location.latitude",
            "location.longitude"
        ],
        "merge_key_1": "id",
        "path_2": "['items'][0]['readings']",
        "fields_2": [
            "station_id",
            "value"
        ],
        "merge_key_2": "station_id"
    },
    "freq_minute": 1,
    "remaining_minute": 1
}
